<?xml version="1.0" encoding="utf-8"?>
<com.cango.palmcartreasure.customview.AutoCardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    app:cardElevation="0dp">
    <LinearLayout
        android:id="@+id/ll_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@drawable/cardview_border">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/tv_staiff_group_name"
                android:layout_width="wrap_content"
                android:layout_height="62px"
                android:layout_marginTop="24px"
                android:layout_marginBottom="24px"
                android:layout_marginLeft="20px"
                android:paddingLeft="31px"
                android:paddingRight="31px"
                android:paddingTop="5px"
                android:paddingBottom="5px"
                android:textSize="46px"
                android:textColor="@android:color/white"
                android:text="@string/unfinished"
                android:background="@drawable/admin_task_group_name_bg"/>
            <TextView
                android:id="@+id/tv_crew_size"
                android:layout_width="wrap_content"
                android:layout_height="62px"
                android:layout_alignParentRight="true"
                android:layout_marginTop="24px"
                android:layout_marginBottom="24px"
                android:layout_marginRight="20px"
                android:padding="5px"
                android:gravity="center"
                android:textSize="45px"
                android:textColor="@color/mt646464"
                android:text="1"
                android:background="@drawable/oval_bg"/>
        </RelativeLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15px"
            android:layout_marginLeft="30px"
            android:layout_marginRight="30px"
            android:layout_marginBottom="20px">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="37px"
                android:textColor="@color/mt848484"
                android:text="@string/group_leader"/>
            <TextView
                android:id="@+id/tv_lead_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:textSize="37px"
                android:textColor="@color/mt848484"
                android:text="@string/group_leader"/>
        </RelativeLayout>
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="1px"
            android:layout_marginLeft="20px"
            android:layout_marginRight="20px"
            android:background="@color/line"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew1"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:layout_height="56px"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew2"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:layout_height="56px"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew3"
                    android:layout_width="wrap_content"
                    android:layout_height="56px"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew4"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:layout_height="56px"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew5"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:layout_height="56px"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginTop="27px"
                android:layout_marginBottom="27px"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_crew6"
                    android:layout_width="wrap_content"
                    android:gravity="center"
                    android:minWidth="70dp"
                    android:layout_height="56px"
                    android:paddingLeft="31px"
                    android:paddingRight="31px"
                    android:paddingTop="5px"
                    android:paddingBottom="5px"
                    android:textSize="40px"
                    android:textColor="@color/mt848484"
                    android:text="@string/unfinished"
                    android:background="@drawable/admin_task_member_name_bg"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</com.cango.palmcartreasure.customview.AutoCardView>